{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev:frontend": {
      "cache": false,
      "dependsOn": ["ui-library#build"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "start": {
      "cache": true
    },
    "dev": {
      "cache": false
    }
  }
}
